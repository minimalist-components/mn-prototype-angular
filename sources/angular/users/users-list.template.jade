.layout-container
  h2 Users list

  .mn-table-list.mn-card
    button.mn-button.action.raised: i.zmdi.zmdi-plus
    a.mn-item(ui-sref='app.users.edit({id: user.id})' ng-repeat='user in users.list')
      span {{ :: user.name }}
      span {{ :: user.email }}

  div.layout-row.align-end
    button.mn-button.action(aria-label='Prev' disabled): i.zmdi.zmdi-chevron-left
    | 1 of 3
    button.mn-button.action(aria-label='Next'): i.zmdi.zmdi-chevron-right
